#include <stdio.h>

int findMax(int arr[], int size) {
    int max = arr[0];
    for (int i = 1; i < size; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }
    return max;
}

int main() {
    int arr[] = {3, 1, 7, 0, 5};
    int size = sizeof(arr) / sizeof(arr[0]);
    printf("Maximum element in array: %d\n", findMax(arr, size));
    printf("swayam");
    return 0;
}
